{
  "name": "golang-sqlx-file-setup",
  "displayName": "golang-sqlx-file-setup",
  "description": "VSCode extension for setting up Golang projects with SQLx.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:golang-sqlx-file-setup.setupProject"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "golang-sqlx-file-setup.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "golang-sqlx-file-setup.setupProject",
        "title": "Setup Golang Project"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "publisher": "AkshatGupta",
  "repository": {
    "type": "git",
    "url": "https://github.com/Oik17/golang-sqlx-file-setup-extension"
  },
  "license": "MIT"

}
